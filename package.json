{
  "name": "@upfluence/ember-upf-utils",
  "version": "5.19.0",
  "description": "The default blueprint for ember-cli addons.",
  "repository": "https://github.com/upfluence/ember-upf-utils",
  "license": "MIT",
  "author": "",
  "keywords": [
    "ember-addon"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test --silent -r dot",
    "test:ember-coverage": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "install-pre-push-hook": "cp ./scripts/pre-push ./.git/hooks/pre-push",
    "remove-pre-push-hook": "rm -rf ./.git/hooks/pre-push",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "dependencies": {
    "@ember/string": "^3.1.1",
    "@embroider/macros": "^1.16.1",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@upfluence/ember-brand-manager": "^1.0.4",
    "@upfluence/hyperevents": "^0.3.8",
    "calculate-cache-key-for-tree": "^2.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-less": "^2.0.6",
    "ember-cli-typescript": "^4.2.1",
    "ember-named-blocks-polyfill": "^0.2.4",
    "moment": "^2.29.4",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "@upfluence/oss-components": "^3.x"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.9.4",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember-data": "^3.16.15",
    "@types/ember-data__adapter": "^3.16.1",
    "@types/ember-data__model": "^3.16.2",
    "@types/ember-data__serializer": "^3.16.1",
    "@types/ember-data__store": "^3.16.1",
    "@types/ember-qunit": "^5.0.2",
    "@types/ember__application": "^3.16.3",
    "@types/ember__array": "^3.16.4",
    "@types/ember__component": "^3.16.6",
    "@types/ember__controller": "^3.16.6",
    "@types/ember__debug": "^3.16.5",
    "@types/ember__engine": "^3.16.3",
    "@types/ember__error": "^3.16.1",
    "@types/ember__object": "^3.12.6",
    "@types/ember__polyfills": "^3.12.1",
    "@types/ember__routing": "^3.16.15",
    "@types/ember__runloop": "^3.16.3",
    "@types/ember__service": "^3.16.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.1",
    "@types/ember__test": "^3.16.1",
    "@types/ember__utils": "^3.16.2",
    "@types/google.maps": "^3.54.6",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/parser": "^5.0.0",
    "@upfluence/oss-components": "^3.81.1",
    "ember-cli": "~3.28.6",
    "ember-cli-code-coverage": "^3.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "upfluence/ember-cli-typescript-blueprints#ember-4-update",
    "ember-data": "~3.18.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-intl": "^6.4.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.1.0",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "typescript": "^4.4.4",
    "webpack": "5"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
